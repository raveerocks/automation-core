<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Google Search Test Suit"  thread-count="4" parallel="tests">
    <parameter name="test-data" value="local-test-data.xlsx"></parameter>
    <test verbose="2" name="Single Test">
        <classes>
            <class name="io.raveerocks.google.SingleTest">
                <methods>
                    <include name="test"/>
                </methods>
            </class>
        </classes>
    </test>
    <test verbose="2" name="Parallel Threads Test">
        <classes>
            <class name="io.raveerocks.google.ParallelThreadTest">
                <methods>
                    <include name="test"/>
                </methods>
            </class>
        </classes>
    </test>
    <test verbose="2" name="Executor Threads Test">
        <classes>
            <class name="io.raveerocks.google.ExecutorThreadTest">
                <methods>
                    <include name="test"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>